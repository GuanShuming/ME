#include<stdio.h>
int main(){
	double a,c;
	int b;
	scanf("%lf",&a);
	if(a >=0){
		b = (int)a;
		c = a - b;
		if(c >= 0.5){
			b = b + 1;
			printf("%d",b);
		}
		else{
			printf("%d",b);
		}
	}
	else{
		b = (int)a;
		c = a - b;
		if(c >= -0.5){
		printf("%d",b);
		}
		else{
			b = b - 1;
			printf("%d",b);
		}
	}
	return 0;
}
